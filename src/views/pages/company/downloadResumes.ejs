<div class="content-wrapper">
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Resume Download</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item active">
                            <a href="/admin/company">Company list</a>
                        </li>
                    </ol>
                </div>
            </div>
        </div>
    </section>

    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-header">
                            <div class="row">
                                <div class="col-10">
                                    <form action="/admin/download-submitted-resumes/" method="get">
                                        <div class="row">
                                            <div class="col-4">
                                                <div class="input-group">
                                                    <input type="number" class="form-control form-control-lg" name="pin_code" value="<%= pin_code %>" placeholder="Pincode" />
                                                    <div class="input-group-append">
                                                        <button type="submit" class="btn btn-lg btn-default">
                              <i class="fa fa-search"></i>
                            </button>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-4">
                                                <div class="input-group">
                                                    <input type="text" class="form-control form-control-lg" name="designation" value="<%= designation %>" placeholder="Designation" />
                                                    <div class="input-group-append">
                                                        <button type="submit" class="btn btn-lg btn-default">
                              <i class="fa fa-search"></i>
                            </button>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-4">
                                                <div class="input-group">
                                                    <select class="form-control form-control-lg" style="font-size: 17px; color: grey" name="experience">
                            <option value="" selected>Select Experience</option>
                            <option value="0">Yes</option>
                            <option value="1">No</option>
                          </select>
                                                    <div class="input-group-append">
                                                        <button type="submit" class="btn btn-lg btn-default">
                              <i class="fa fa-search"></i>
                            </button>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="mt-2 col-4">
                                                <div class="input-group">
                                                    <select class="form-control form-control-lg" style="font-size: 17px; color: grey" name="state_id">
                            <option value="" selected>Select State</option>
                            <% if(states) states.forEach(oneStates =>{ %>
                            <option value="<%= oneStates.id %>">
                              <%= oneStates.name %>
                            </option>
                            <% }) %>
                          </select>
                                                    <div class="input-group-append">
                                                        <button type="submit" class="btn btn-lg btn-default">
                              <i class="fa fa-search"></i>
                            </button>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="mt-2 col-4">
                                                <div class="input-group">
                                                    <select class="form-control form-control-lg" style="font-size: 17px; color: grey" name="city_id">
                            <option value="" selected>Select City</option>
                            <% if(cities) cities.forEach(onecities =>{ %>
                            <option value="<%= onecities.id %>">
                              <%= onecities.name %>
                            </option>
                            <% }) %>
                          </select>
                                                    <div class="input-group-append">
                                                        <button type="submit" class="btn btn-lg btn-default">
                              <i class="fa fa-search"></i>
                            </button>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="mt-2 col-4">
                                                <div class="input-group">
                                                    <select class="form-control form-control-lg" style="font-size: 17px; color: grey" name="Skills_id">
                            <option value="" selected>Select Skills</option>
                            <% if(Skills) Skills.forEach(oneSkill =>{ %>
                            <option value="<%= oneSkill.id %>">
                              <%= oneSkill.name %>
                            </option>
                            <% }) %>
                          </select>
                                                    <div class="input-group-append">
                                                        <button type="submit" class="btn btn-lg btn-default">
                              <i class="fa fa-search"></i>
                            </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                        <div>
                            <% if(message != 0) { %>
                                <div class="p-3">
                                    <div class="alert mb-0" style="
                    background-color: rgb(188 252 188);
                    border: none;
                    color: rgb(2, 120, 28);
                  " role="alert">
                                        <%= message %>
                                    </div>
                                </div>
                                <% } else if(error != 0){ %>
                                    <div class="p-3">
                                        <div class="alert mb-0" style="
                    background-color: rgb(237 183 183);
                    border: none;
                    color: rgb(120 2 2);
                  " role="alert">
                                            <%= error %>
                                        </div>
                                    </div>
                                    <% } %>
                        </div>

                        <% if(countData != 0) { %>

                            <div class="card-body">
                                <table id="example2" class="table table-bordered table-hover">
                                    <thead>
                                        <tr>
                                            <th>S no.</th>
                                            <th>Name</th>
                                            <th>Email</th>
                                            <th>Pincode</th>
                                            <th>State</th>
                                            <th>City</th>
                                            <th>Skills</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% resumeData.rows.forEach((value,index)=>{ %>
                                            <tr>
                                                <td>
                                                    <%=index+1%>
                                                </td>
                                                <td>
                                                    <%=value.name%>
                                                </td>
                                                <td>
                                                    <%=value.email%>
                                                </td>
                                                <td>
                                                    <%=value.pin_code%>
                                                </td>
                                                <td>
                                                    <%=value.state?value.state.name:''%>
                                                </td>
                                                <td>
                                                    <%=value.city?value.city.name:''%>
                                                </td>
                                                <td>
                                                    <% value.resume_skills.forEach((val,index)=>{ %>
                                                        <%=val.SkillSubCategory.name%> ,
                                                            <%})%>
                                                </td>
                                            </tr>
                                            <%})%>
                                    </tbody>
                                </table>
                            </div>
                            <% } %>
                                <div class="col-sm-2 m-2 col-4 justify-content-start mt-5">
                                    <div class="row" style="width: 490px; justify-content: space-between">
                                        <a href="/admin/resume-download?type=excel&<%= str%>">
                                            <div class="btn btn-sm bg-info">
                                                Download Candidate Details as Excel
                                            </div>
                                        </a>
                                        <a href="/admin/resume-download?<%= str%>">
                                            <div class="btn btn-sm bg-info">
                                                Download Candidate Details as PDF
                                            </div>
                                        </a>
                                    </div>
                                </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>