                        <div>
                            <% if(message != 0) { %>
                                <div class="p-3">
                                    <div class="alert mb-0" style="background-color: rgb(188 252 188); border: none; color: rgb(2, 120, 28);" role="alert">
                                        <%= message %>
                                    </div>
                                </div>

                                <% } else if(error != 0){ %>
                                    <div class="p-3">
                                        <div class="alert mb-0" style="background-color: rgb(237 183 183); border: none; color: rgb(120 2 2);" role="alert">
                                            <%= error %>
                                        </div>
                                    </div>
                                    <% } %>
                        </div>

                        <% if(data.rows.length > 0) { %>
                            <table id="example2" class="table table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th>S no.</th>
                                        <th>Reason</th>
                                        <th>Date</th>
                                        <th>Commission Recieved</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% data.rows.forEach((dets, index)=> { %>
                                        <tr>
                                            <%= console.log(dets) %>
                                                <td>
                                                    <%= index + 1%>
                                                </td>
                                                <td>
                                                    <%= dets.dataValues.Reason  %>
                                                </td>
                                                <td>
                                                    <%= dets.dataValues.date  %>
                                                </td>
                                                <td>
                                                    <%= dets.dataValues.my_commission_amount  %>
