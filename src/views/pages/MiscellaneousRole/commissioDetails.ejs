                                    <div class="btn btn-sm bg-info">
                                        Download Commision Details Excel
                                    </div>
                                </a>
                                &nbsp;
                                <a href="/admin/commission-details-download/<%= user_id %>">
                                    <div class="btn btn-sm bg-info">
                                        Download Commision Details PDf
                                    </div>
                                </a>
                            </div>

                            <div class="col-sm-2 m-2 col-4 paginationWrapper ml-auto justify-content-end" style="padding-right: 1rem;">
                                <% if (extra.pages > 0) { %>
                                    <ul class="pagination d-flex justify-content-end">
                                        <% var i=(Number(extra.pageNo) > 5 ? Number(extra.pageNo) - 4 : 1) %>
                                            <% if (i !==1) { %>
                                                <li class="page-item disabled">
                                                    <a class="page-link">...</a></li>
                                                <% } %>
                                                    <% for (; i <=(Number(extra.pageNo) + 4) && i <=extra.pages; i++) { %>


                                                        <% if (i==extra.pageNo) { %>
                                                            <li class="page-item active">
                                                                <a class="page-link">
                                                                    <%= i %>
                                                                </a>
                                                            </li>
                                                            <% } else { %>
                                                                <li class="page-item">
                                                                    <a class="page-link" href="/admin/miscellaneous-commission-details/<%= user_id %>?page=<%= i %>">
                                                                        <%= i %>
                                                                    </a>
                                                                </li>
                                                                <% } %>

                                                                    <% if (i==Number(extra.pageNo) + 4 && i < extra.pages) { %>
                                                                        <li class="page-item disabled">
                                                                            <a class="page-link">...</a></li>
                                                                        <% } %>
                                                                            <% } %>
                                    </ul>
                                    <% } %>
