<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Subscribed Users</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item active">
                            <a href="/admin/subscription-plan-details/<%= id %>">Plan Details</a
              >
            </li>
          </ol>
        </div>
      </div>
    </div>
    <!-- /.container-fluid -->
  </section>

  <!-- Main content -->
  <section class="content">
    <div class="container-fluid">
      <div class="row">
        <div class="col-12">
          <div class="card">
            <div class="card-header">
              <h1>Subscribed Users</h1>
            </div>
            <!-- /.card-header -->

            <div>
              <% if(message !='' ) { %>
              <div class="p-3">
                <div
                  class="alert mb-0"
                  style="
                    background-color: rgb(141, 228, 141);
                    border: none;
                    color: rgb(2, 120, 28);
                  "
                  role="alert"
                >
                  <%= message %>
                </div>
              </div>

              <% } else if(error !='' ){ %>
              <div class="p-3">
                <div
                  class="alert mb-0"
                  style="
                    background-color: rgb(233, 153, 153);
                    border: none;
                    color: rgb(120 2 2);
                  "
                  role="alert"
                >
                  <%= error %>
                </div>
              </div>
            </div>
            <% } else { %>

            <div>
              <div class="card-body">
                <table id="example2" class="table table-bordered table-hover">
                  <thead>
                    <tr>
                      <th>S no.</th>
                      <th>User Name</th>
                      <th>Email</th>
                      <th>Start Date</th>
                      <th>Expiry Date</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% data.rows.forEach((objData, index)=> { %>
                    <tr>
                      <td><%= index+1%></td>
                      <td><%= objData.subscribed_user[0].name%></td>
                      <td><%= objData.subscribed_user[0].email%></td>
                      <td><%= moment(objData.start_date).format('DD-MM-YYYY')%></td>
                      <td><%= moment(objData.expiry_date).format('DD-MM-YYYY')%></td>
                    </tr>
                    <%} ) %>
                  </tbody>
                </table>
              </div>
            </div>

            <% }%>

            <div class="paginationWrapper" style="padding-right: 1rem">
              <% if (extra.pages> 0) { %>
              <ul class="pagination d-flex justify-content-end">
                <% var i=(Number(extra.pageNo)> 5 ? Number(extra.pageNo) - 4 :
                1) %> <% if (i !==1) { %>
                <li class="page-item disabled">
                  <a class="page-link">...</a>
                        </li>
                        <% } %>
                            <% for (; i <=(Number(extra.pageNo) + 4) && i
                <=extra.pages; i++) { %>
                                <% if (i==extra.pageNo) { %>
                                    <li class="page-item active">
                                        <a class="page-link">
                                            <%= i %>
                                        </a>
                                    </li>
                                    <% } else { %>
                                        <li class="page-item">
                                            <a class="page-link" href="/admin/subscription-plan/<%= id %>/subscribed-users?page=<%= i %>">
                                                <%= i %>
                                            </a>
                                        </li>
                                        <% } %>
                                            <% if (i==Number(extra.pageNo) + 4 && i < extra.pages) {
                %>
                                                <li class="page-item disabled">
                                                    <a class="page-link">...</a>
                                                </li>
                                                <% } %>
                                                    <% } %>
                                                        </ul>
                                                        <% } %>
                </div>
                <!-- /.card-body -->
            </div>
            <!-- /.card -->

            <!-- /.card -->
        </div>
        <!-- /.col -->
</div>
<!-- /.row -->
</div>
<!-- /.container-fluid -->
</section>
<!-- /.content -->
</div>